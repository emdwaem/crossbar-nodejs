{
    "get_recordings": {
        "method": "GET",
        "url": "/accounts/{{account_id}}/recordings{{filters}}",
        "auth_required": true
    },
    "get_recording": {
        "method": "GET",
        "url": "/accounts/{{account_id}}/recordings/{{recording_id}}",
        "auth_required": true
    },
    "delete_recording": {
        "method": "DELETE",
        "url": "/accounts/{{account_id}}/recordings/{{recording_id}}",
        "auth_required": true
    }
}